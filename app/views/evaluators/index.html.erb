<div class="main-evaluador">
<div class="barra-evaluador">
    <p class="letra-barra-evaluador">Evaluadores</p>
 </div>
<p id="notice"><%= notice %></p>
<div class="index-evaluador">
<a class="btn waves-effect waves-light btn-atras grey right" style="margin-bottom: 30px" <%= link_to 'Nuevo', new_evaluator_path %> </a>
<table>
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Especialidad</th>
      <th>Fecha de Registro</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluators.each do |evaluator| %>
      <tr>
        
        <td><%= evaluator.name %></td>
        <td><%= evaluator.lastname %></td>
        <td><%= evaluator.specialty %></td>
        <td><%= evaluator.registration_date %></td>
        <td><%= link_to 'Show', evaluator %></td>
        <td><%= link_to 'Edit', edit_evaluator_path(evaluator) %></td>
        <td><%= link_to 'Destroy', evaluator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


